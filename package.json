{
  "name": "backup-n8n",
  "version": "1.0.0",
  "description": "Backup all workflows from an n8n instance via CLI.",
  "bin": {
    "backup-n8n": "bin/n8n-backup.js"
  },
  "type": "commonjs",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "n8n",
    "backup",
    "cli",
    "workflows"
  ],
  "author": "",
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js --help",
    "prepare": "npm run build",
    "test": "vitest run --reporter=verbose",
    "test:coverage": "vitest run --reporter=verbose --coverage"
  },
  "dependencies": {
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "@types/yauzl": "^2.10.3",
    "@vitest/coverage-v8": "^1.6.1",
    "dotenv": "^16.4.5",
    "vitest": "^1.6.0",
    "yauzl": "^2.10.0"
  },
  "vitest": {
    "coverage": {
      "provider": "v8",
      "reporter": [
        "text",
        "html",
        "lcov"
      ],
      "reportsDirectory": "test/coverage",
      "reportOnFailure": true,
      "all": false,
      "include": [
        "src/**/*"
      ]
    }
  }
}